//API12BAT JOB (ACCT#),'COMPDB2',MSGCLASS=H,REGION=4M,
//    CLASS=A,MSGLEVEL=(1,1),NOTIFY=&SYSUID,
//    COND=(4,LT),TIME=(0,30)
//PROCLIB  JCLLIB ORDER=SDJ.FORM.PROCLIB
//*
//         SET SYSUID=API12,
//             NOMPGM=PROBATC
//APPROC   EXEC COMPDB2
//STEPDB2.SYSLIB   DD DSN=&SYSUID..SOURCE.DCLGEN,DISP=SHR
//STEPDB2.SYSIN    DD DSN=&SYSUID..SOURCE.DB2(&NOMPGM),DISP=SHR
//STEPDB2.DBRMLIB  DD DSN=&SYSUID..SOURCE.DBRMLIB(&NOMPGM),DISP=SHR
//STEPLNK.SYSLMOD  DD DSN=&SYSUID..SOURCE.PGMLIB(&NOMPGM),DISP=SHR
//STEPLNK.SYSLIB  DD DSN=API12.SOURCE.LOAD,DISP=SHR
//                DD DSN=CICSTS22.CICS.SDFHEXCI,DISP=SHR
//                DD DSN=CICSTS22.CICS.SDFHLOAD,DISP=SHR
//                DD DSN=DSN710.SDSNLOAD,DISP=SHR
//                DD DSN=CEE.SCEECICS,DISP=SHR
//                DD DSN=CEE.SCEELKED,DISP=SHR
//STEPLNK.SYSIN   DD *
 INCLUDE SYSLIB(LOGDATA)
/*
//**********************************************
//*----           BIND        -----
//***********************************************
//BIND     EXEC PGM=IKJEFT01,COND=(4,LT)
//DBRMLIB  DD  DSN=&SYSUID..SOURCE.DBRMLIB,DISP=SHR
//SYSTSPRT DD  SYSOUT=*,OUTLIM=25000
//SYSTSIN  DD  *
  DSN SYSTEM (DSN1)
  BIND PLAN      (PROBATC)    -
       QUALIFIER (API12)      -
       ACTION    (REPLACE)    -
       MEMBER    (PROBATC)    -
       VALIDATE  (BIND)       -
       ISOLATION (CS)         -
       ACQUIRE   (USE)        -
       RELEASE   (COMMIT)     -
       EXPLAIN   (NO)
/*
//STEPRUN  EXEC PGM=IKJEFT01,COND=(4,LT)
//STEPLIB  DD  DSN=&SYSUID..SOURCE.PGMLIB,DISP=SHR
//SYSOUT   DD  SYSOUT=*,OUTLIM=1000
//SYSTSPRT DD  SYSOUT=*,OUTLIM=2500
//STATDATA DD  DSN=&SYSUID..OUTPUT.STATS,DISP=OLD,
//             UNIT=SYSDA,SPACE=(CYL,(1,1)),RECFM=FB,LRECL=200
//LOG      DD  DSN=API12.PROJET.LOGDATA.DATA,DISP=SHR
//SORTWK   DD  DSN=API12.PROJET.TEMP.DATA,SPACE=(CYL,(10,5)),
//             DISP=(NEW,DELETE)
//SYSTSIN  DD  *
 DSN SYSTEM (DSN1)
 RUN PROGRAM(PROBATC) PLAN (PROBATC)
/*
